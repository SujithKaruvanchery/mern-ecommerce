{
    "rewrites": [
      { "source": "/api/*", "destination": "https://your-backend-url.com/api/:path*" }
    ],
    "redirects": [
      { "source": "/old-route", "destination": "/new-route", "permanent": true }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  